{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./app/styles/global.css -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/global.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "dev:zeus": "npx zeus http://localhost:4444/api/graphql ./graphql-zeus",
    "start": "remix-serve build"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "D": "^1.0.0",
    "dotenv": "^11.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql-zeus": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "zod": "^3.12.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@remix-run/eslint-config": "^1.1.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.7.0",
    "eslint-config-kentcdodds": "^20.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build",
    "server_build"
  ],
  "sideEffects": false
}
